{
  "name": "@volcengine/sdk-monorepo",
  "version": "1.0.0",
  "description": "VolcEngine SDK monorepo",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "service/*",
    "tools/*",
    "examples"
  ],
  "scripts": {
    "build": "turbo run build --filter='./packages/sdk-core' --filter='./service/*'",
    "build:core": "turbo run build --filter='./packages/sdk-core'",
    "build:service": "turbo run build --filter='./service/*'",
    "test": "turbo run test",
    "test:core": "turbo run test --filter='./packages/*'",
    "clean": "turbo run clean",
    "release": " pnpm publish -r --access public --filter='./packages/*' --filter='./service/*'",
    "release:core": "pnpm publish -r --access public --filter='./packages/*' --no-git-checks",
    "release:service": "pnpm publish -r --access public --filter='./service/*' --no-git-checks",
    "release:iam": " pnpm publish -r --access public --filter='./service/iam' --no-git-checks"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@rslib/core": "^0.19.1",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.10.0"
}
