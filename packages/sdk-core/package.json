{
  "name": "@volcengine/sdk-core",
  "version": "1.0.0",
  "description": "VolcEngine SDK core package",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./testing": {
      "types": "./dist/esm/testing.d.ts",
      "import": "./dist/esm/testing.mjs",
      "require": "./dist/cjs/testing.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rslib build",
    "test": "jest --maxWorkers=2",
    "clean": "rm -rf dist",
    "dev": "rslib build --watch"
  },
  "dependencies": {
    "@volcengine/openapi": "^1.34.0",
    "axios": "^1.6.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.2.2"
  },
  "sideEffects": false
}
